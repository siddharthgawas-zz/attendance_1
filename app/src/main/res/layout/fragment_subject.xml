<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.pccegoa.studentapp.fragment.SUBJECT">


    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="120sp"
        android:layout_height="120sp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="336dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:src="@drawable/at"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.662"
        app:layout_constraintStart_toEndOf="@+id/imageView15" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="120dp"
        android:layout_height="120sp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView9"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/at"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView13"
        app:layout_constraintVertical_bias="0.277" />


    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="120sp"
        android:layout_height="120sp"
        android:layout_alignStart="@+id/imageView10"
        android:layout_below="@+id/imageView10"
        android:layout_marginEnd="32dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/at"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="120sp"
        android:layout_height="120sp"
        android:layout_alignStart="@+id/imageView9"
        android:layout_alignTop="@+id/imageView11"
        android:layout_marginStart="48dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/at"
        android:clickable="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView15" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="120sp"
        android:layout_height="120sp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/imageView13"
        android:layout_marginBottom="60dp"
        android:layout_marginEnd="32dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/at"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="120sp"
        android:layout_height="120sp"
        android:layout_alignStart="@+id/imageView11"
        android:layout_alignTop="@+id/imageView14"
        android:layout_marginBottom="336dp"
        android:layout_marginStart="48dp"
        android:src="@drawable/at"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView9"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="16dp"

        android:layout_marginTop="8dp"
        android:text="Number of Classes Attended:"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.714" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView10"
        android:layout_alignStart="@+id/imageView9"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="16dp"
        android:text="Number of Classes Missed:"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView15"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/attendedclasses"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView9"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="12dp"
        android:layout_toEndOf="@+id/textView8"
        android:ems="10"
        android:inputType="number"
        android:text="64"
        app:layout_constraintBottom_toTopOf="@+id/imageView9"
        app:layout_constraintStart_toEndOf="@+id/textView9" />

    <TextView
        android:id="@+id/Missedclasses"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/attendedclasses"
        android:layout_below="@+id/attendedclasses"
        android:layout_marginBottom="24dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="number"
        android:text="54"
        app:layout_constraintBottom_toTopOf="@+id/attendedclasses"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.379"
        app:layout_constraintStart_toStartOf="@+id/attendedclasses"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.941" />




</android.support.constraint.ConstraintLayout>
